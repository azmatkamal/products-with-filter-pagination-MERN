{
  "name": "The-teams",
  "version": "1.0.0",
  "description": "The teams CRM built using nodejs and reactjs",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --only=dev --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "nodejs",
    "jwt",
    "passport",
    "express"
  ],
  "author": "Azmat Kamal",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.25.1",
    "compression": "^1.7.4",
    "concurrently": "^6.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.2.0",
    "mongodb": "^3.1.6",
    "mongoose": "^5.4.19",
    "mongoose-paginate-v2": "^1.3.18",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.4.17",
    "nodemon": "^1.18.3",
    "normalize-url": "^5.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "util": "^0.11.1",
    "uuid": "^3.4.0",
    "validator": "^9.4.1"
  },
  "engines": {
    "node": "10.x"
  }
}
